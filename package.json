{
  "name": "compose-hooks-react",
  "version": "1.1.2",
  "description": "Compose React hooks together and decouple them from your components",
  "main": "build/compose.js",
  "types": "src/compose.d.ts",
  "scripts": {
    "test": "jest src",
    "test:watch": "jest src --watch",
    "build": "babel src/compose.js --out-file build/compose.js",
    "postinstall": "npm run build",
    "prepublish": "npm run build"
  },
  "keywords": ["react", "hooks", "compose", "decouple", "test"],
  "author": "Eric Andre<ericandre615@gmail.com>",
  "homepage": "https://github.com/ericandre615/react-compose-hooks/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/ericandre615/react-compose-hooks"
  },
  "license": "ISC",
  "peerDependencies": {
    "react": ">= 16.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@testing-library/react": "^11.2.5",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.1",
    "jest": "^26.6.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "regenerator-runtime": "^0.13.7"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest/setup-tests.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules"
    ]
  }
}
